<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="440.0" prefWidth="554.0" spacing="20.0" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.finaltask.lab1.src.Lab1Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Lab â„–1. Move Strategy" />
   <Label alignment="BASELINE_LEFT" prefHeight="17.0" prefWidth="519.0" text="Start pos" />
   <TextField fx:id="startPos" promptText="Enter start position" prefHeight="25.0" prefWidth="87.0" />
   <Label alignment="BASELINE_LEFT" prefHeight="17.0" prefWidth="519.0" text="Modify" />
   <TextField fx:id="startModify" promptText="Enter modify" prefHeight="25.0" prefWidth="87.0" />
   <Label alignment="BASELINE_LEFT" prefHeight="17.0" prefWidth="519.0" text="Strategy" />
   <TextField fx:id="strategyChoice" promptText="Enter your strategy" prefHeight="25.0" prefWidth="87.0" />
   <Label alignment="BASELINE_LEFT" prefHeight="17.0" prefWidth="519.0" text="Result" />
   <TextField fx:id="resultText" alignment="TOP_CENTER" editable="False" prefHeight="25.0" prefWidth="175.0" />
   <BorderPane prefHeight="200.0" prefWidth="200.0">
      <left>
         <Button onAction="#onBackButtonClick" prefHeight="25.0" prefWidth="100.0" text="Back" BorderPane.alignment="CENTER" />
      </left>
      <right>
         <Button onAction="#onExitButtonClick" prefHeight="25.0" prefWidth="100.0" text="Exit" BorderPane.alignment="CENTER" />
      </right>
      <center>
         <Button onAction="#startClickAction" prefHeight="25.0" prefWidth="100.0" text="Start" BorderPane.alignment="CENTER" />
      </center>
   </BorderPane>
</VBox>
